<h1>Time Entries</h1>
<%= link_to 'New Time Entry', new_time_entry_path, class: "btn btn-default"%>
<table class="table table-condensed">
  <thead>
    <tr>
      <th>Time spent</th>
      <th>Date</th>
      <th>Summary</th>
      <th>ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr>
        <td><%= time_entry.time_spent %></td>
        <td><%= time_entry.date %></td>
        <td><%= time_entry.summary %></td>
        <td><%= time_entry.id %></td>
        <td><%= link_to 'Show', time_entry, class: "btn btn-default btn-xs" %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(time_entry), class: "btn btn-warning btn-xs" %></td>
        <td><%= link_to 'Destroy', time_entry, method: :delete, class: "btn btn-danger btn-xs" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>













